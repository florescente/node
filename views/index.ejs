<!DOCTYPE html>
<html lang="en">
<%- include('./partials/head.ejs') %>
<body>

    <h1>Oh my!!</h1>
    <p>Hello beauty</p>
    <%- include('./partials/nav.ejs') %>

    <div class="blog content">
        <h2>All BLogs</h2>
        
        <% if (blogs.length > 0) { %>
            <% blogs.forEach(blog => { %>
                <h3 class="title"><%= blog.title %></h3>
                <p class="snippet"><%= blog.snippet %></p>
            <% }) %>
        <% } %>
    </div>
    <%- include('./partials/foot.ejs') %>
</body>
</html>